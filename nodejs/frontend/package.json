{
  "name": "phaser-game-frontend",
  "version": "1.0.0",
  "description": "Vite + Phaser 4 Game Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit --skipLibCheck && vite build",
    "preview": "vite preview",
    "demo": "cd demo && vite",
    "demo:build": "cd demo && tsc --noEmit --project tsconfig.json && vite build",
    "demo:preview": "cd demo && vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:build": "tsc --project tsconfig.test.json",
    "test:type-check": "tsc --noEmit --project tsconfig.test.json",
    "integration:verify": "tsc --project tsconfig.node.json && node dist/integration/verify-loading-logic.js",
    "integration:test": "jest src/integration/AssetLoadingIntegration.test.ts",
    "integration:manual": "tsc --project tsconfig.node.json && node dist/integration/manual-loading-test.js"
  },
  "dependencies": {
    "phaser": "^4.0.0-beta.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
